@using CourseAplicationLib
@model Fuc

@{
    ViewBag.Title = @Model.Name+" Edit";
}
<a href=@Url.Action("Index", "Home")>Home</a>
<h1>@Model.Name Edit</h1>

    @using (Html.BeginForm("Edit", "Fuc", FormMethod.Post))
    {
        <p>
        @Html.Label("Name")
        @Html.EditorFor(n => Model.Name)
        </p>
        <p>
        @Html.Label("Acr")
        @Html.EditorFor(n => Model.Acr)
        </p>
        <p>
        @Html.Label("Required")
        @Html.EditorFor(n => Model.IsRequired)
        </p>
        <p>
        @Html.Label("Semesters")
        </p>
        <p>
        @Html.Label("Prerequisites")
        </p>
        <p>
        @Html.Label("ECTS")
        @Html.EditorFor(n => Model.Ects)
        </p>
        <p>
        @Html.Label("Objectives: ")
        </p>
        <p>
        @Html.Label("Results: ") 
        </p>
        <p>
        @Html.Label("Evaluation: ")
        </p>
        <p>
        @Html.Label("Program: ")
        </p>
        
         <input type="submit" value="submit" />

    }
@{Html.EndForm();}
                    