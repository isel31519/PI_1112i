@using CourseAplicationMVC.Models
@model UserProfile

@{
    ViewBag.Title = "User Profile";
}

<h1>@Model.UserName Profile</h1>

@Html.LabelFor(n => Model.FirstName)<br/>
@Html.DisplayTextFor(n => Model.FirstName)<br/><br/>

@Html.LabelFor(n => Model.LastName)<br/>
@Html.DisplayTextFor(n => Model.LastName)<br/><br/>

@Html.LabelFor(n => Model.Email)<br/>
@Html.DisplayTextFor(n => Model.Email)<br/><br/>

@Html.LabelFor(n => Model.Roles)<br/>
@Html.DisplayTextFor(n=> Model.Roles) <br/><br/>